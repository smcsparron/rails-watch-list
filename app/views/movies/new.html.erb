<div class="container pt-3">
  <%= form_with model: @movie do |f| %>
    <%= f.label :title %>
    <%= f.text_field :title %>
    <%= f.label :overview %>
    <%= f.text_field :overview %>
    <%= f.label :rating %>
    <%= f.text_field :rating %>
    <%= f.label :poster_url %>
    <%= f.text_field :poster_url %>
    <%= f.submit %>
  <% end %>
</div>
